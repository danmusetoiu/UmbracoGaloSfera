    <div class="container container-fluid text-center">

        <div class="row">
            @* <div class="col-md-6">
                <h2>@Model.MapHeader</h2>
                @Model.MapCoordinates

            </div> *@

            <div class="col-md-6">
                <h2>@Model.ContactFormHeader</h2>
                @Model.ContactIntro
